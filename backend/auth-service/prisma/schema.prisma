generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AuthUser {
  id        String   @id @default(uuid())
  fullName  String
  username  String   @unique
  tcId      String?   @unique
  email     String   @unique
  password  String
  refreshToken String?
  createdAt DateTime @default(now())
}
